{
  "presets": [
    "react",
    "es2015"
  ],

  "plugins": [
    "transform-async-to-generator",
    "transform-class-properties",
    "transform-decorators",
    "transform-object-rest-spread",
    "transform-runtime"
  ],

  "env": {
    "build": {
      "plugins": [
        ["transform-require-ignore", {"extensions": [".less"] }],
        "transform-import-extension-jsx-to-js"
      ]
    }
  }
}
